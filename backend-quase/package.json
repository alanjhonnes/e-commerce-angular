{
  "name": "backend-model",
  "version": "1.0.0",
  "description": "Backend Modelo",
  "main": "index.js",
  "scripts": {
    "start": "ts-node src/index.ts",
    "watch": "ts-node-dev src/index.ts --debug",
    "codegen": "graphql-codegen --config ./codegen.yml",
    "codegen:watch": "npm run codegen -- --watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "migrate": "knex migrate:latest",
    "migrate:make": "knex migrate:make -x ts",
    "migrate:rollback": "knex migrate:rollback",
    "migrate:rollback-all": "knex migrate:rollback --all",
    "seed:make": "knex seed:make -x ts",
    "seed:run": "knex seed:run"
  },
  "author": "Alex Botelho <alexbotelho1@gmail.com>",
  "license": "UNLICENSE",
  "dependencies": {
    "@graphql-codegen/cli": "^1.17.0",
    "@graphql-codegen/typescript": "^1.17.0",
    "@graphql-codegen/typescript-resolvers": "^1.17.0",
    "@types/ramda": "^0.27.11",
    "dataloader": "^2.0.0",
    "dotenv": "^8.2.0",
    "fastify": "^3.19.2",
    "fastify-cors": "^6.0.2",
    "fastify-express": "^0.3.3",
    "fastify-jwt": "^3.0.0",
    "fastify-redis": "^4.3.1",
    "fp-ts": "^2.7.0",
    "graphql": "^15.3.0",
    "graphql-tag": "^2.10.4",
    "graphql-tools": "^6.0.14",
    "knex": "^0.21.12",
    "mercurius": "^8.1.0",
    "mysql2": "^2.1.0",
    "ramda": "^0.27.0",
    "typescript": "^3.9.7"
  },
  "devDependencies": {
    "@types/ioredis": "^4.26.6",
    "@types/node": "^14.0.23",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "eslint": "^7.4.0",
    "jest": "^26.1.0",
    "ts-node": "^10.1.0",
    "ts-node-dev": "^1.0.0-pre.52"
  }
}
